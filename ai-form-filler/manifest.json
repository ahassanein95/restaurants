{
  "manifest_version": 3,
  "name": "AI Form Filler",
  "version": "1.0.0",
  "description": "Intelligently fill out forms using AI",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "https://api.openai.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "AI Form Filler"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["config.js", "user-profile.js", "ai-service.js", "content.js"],
      "run_at": "document_end"
    }
  ]
} 